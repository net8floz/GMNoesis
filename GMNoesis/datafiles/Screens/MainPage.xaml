<Page
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:noesis="clr-namespace:NoesisGUIExtensions"
  xmlns:system="clr-namespace:System;assembly=mscorlib"
  Height="NaN"
  d:DataContext="ViewModel Data 01"
  d:DesignHeight="720"
  d:DesignWidth="1280"
  mc:Ignorable="d">
  <b:Interaction.Triggers>
    <noesis:DataEventTrigger EventName="play_reward_animation" Source="{Binding} ">
      <b:ControlStoryboardAction Storyboard="{DynamicResource Storyboard0}"/>
    </noesis:DataEventTrigger>
  </b:Interaction.Triggers>
  <Page.Resources >
    <Storyboard x:Key="Storyboard0"/>
  </Page.Resources>
  <TextBlock TextWrapping="Wrap" Text="HELLO WORLD" Foreground="#FF0884FF"/>
</Page>
